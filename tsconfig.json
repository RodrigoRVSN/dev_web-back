{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "es5",
    "outDir": "dist",
    "rootDirs": [
      "./src",
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true
  }
}